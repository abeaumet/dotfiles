{
  "editor.fontFamily": "FuraCode Nerd Font",
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "workbench.startupEditor": "none",
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.rulers": [
    80
  ],
  "editor.fontLigatures": true,
  "editor.cursorBlinking": "solid",
  "editor.dragAndDrop": false,
  "workbench.editor.tabCloseButton": "off",
  "window.title": "${folderName}",
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/coverage/**": false
  },
  "window.zoomLevel": 0,
  "terminal.integrated.fontSize": 14,
  "typescript.npm": "/usr/local/bin/npm",
  "explorer.confirmDragAndDrop": false,
  "search.useIgnoreFiles": true,
  "extensions.ignoreRecommendations": false,
  "eslint.enable": true,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[terraform]": {
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.formatOnSave": true
  },
  "editor.trimAutoWhitespace": true,
  "files.trimTrailingWhitespace": true,
  "workbench.statusBar.feedback.visible": false,
  "explorer.confirmDelete": false,
  "files.hotExit": "off",
  "workbench.tree.horizontalScrolling": true,
  "editor.codeLens": false,
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "editor.formatOnPaste": true,
  "breadcrumbs.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotionKeys": "tnseriaodhplfuwyq;gjvmc,x.z/bk",
  "yaml.format.enable": false,
  "scm.diffDecorations": "none",
  "rust-client.updateOnStartup": true,
  "rust.clippy_preference": "on",
  "eslint.packageManager": "yarn",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.associations": {
    "*.rustpeg": "peg",
    "*.ho": "go",
    "*.bo": "go"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "workbench.activityBar.visible": false,
  "rust.all_features": true,
  "editor.fontWeight": "400",
  "editor.accessibilitySupport": "off",
  "rust.unstable_features": true,
  "rust.wait_to_build": 500,
  "workbench.sideBar.location": "left",
  "liveshare.featureSet": "insiders",
  "gitblame.commitUrl": "Blame guess",
  "gitblame.ignoreWhitespace": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "editor.snippetSuggestions": "none",
  "go.autocompleteUnimportedPackages": true,
  "go.gotoSymbol.includeImports": true,
  "go.useLanguageServer": true,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.formatTool": "goimports",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.easymotionMarkerFontFamily": "FuraCode Nerd Font",
  "vim.easymotionMarkerForegroundColorOneChar": "red",
  "vim.easymotionMarkerForegroundColorTwoChar": "yellow",
  "vim.easymotionMarkerYOffset": 3,
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerHeight": 18,
  "vim.easymotionMarkerWidthPerChar": 7,
  "debug.node.autoAttach": "on",
  "terminal.integrated.fontFamily": "FuraCode Nerd Font",
  "vim.easymotionMarkerBackgroundColor": "#1E1E1E",
  "workbench.editor.enablePreview": false,
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.suggestionActions.enabled": false,
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.surveys.enabled": false
}
